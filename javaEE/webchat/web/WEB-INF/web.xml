<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>com.servlet.Main</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Loginfail</servlet-name>
        <servlet-class>com.servlet.Loginfail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Content</servlet-name>
        <servlet-class>com.servlet.Content</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>counter</servlet-name>
        <servlet-class>com.servlet.counter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>counterListener</servlet-name>
        <servlet-class>com.servlet.counterListener</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>time</servlet-name>
        <servlet-class>com.servlet.time</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>webchat</servlet-name>
        <servlet-class>com.servlet.webchat</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/Main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Loginfail</servlet-name>
        <url-pattern>/Loginfail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Content</servlet-name>
        <url-pattern>/Content</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>counter</servlet-name>
        <url-pattern>/counter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>counterListener</servlet-name>
        <url-pattern>/counterListener</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>time</servlet-name>
        <url-pattern>/time</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>webchat</servlet-name>
        <url-pattern>/webchat</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
